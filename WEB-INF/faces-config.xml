<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.1"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">


    <navigation-rule>
    <from-view-id>/welcomeJSF.jsp</from-view-id>
      <navigation-case>
          <from-outcome>Inserir</from-outcome>
          <to-view-id>/newjsf.xhtml</to-view-id>
          </navigation-case>
  </navigation-rule>   
     
     <navigation-rule>
    <from-view-id>/newjsf.xhtml</from-view-id>
      <navigation-case>
          <from-outcome>Remover</from-outcome>
          <to-view-id>/welcomeJSF.jsp</to-view-id>
      </navigation-case>
  </navigation-rule>
  
  
   <navigation-rule>
    <from-view-id>/InserriEstabelecimento.jsp</from-view-id>
      <navigation-case>
          <from-outcome>Inserir</from-outcome>
          <to-view-id>/ListarEstabelecimentos.xhtml</to-view-id>
       </navigation-case>
  </navigation-rule>
  
</faces-config>
